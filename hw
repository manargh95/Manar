<html> <body>
	class Person {
private $name;
private $age;
private $tall;
public function setName($n){ $this->name = $n;}
public function setAge($g) { $this->age = $g;}
public function setTall($t) {$this->tall = $t;}

public function getName($n){return $this->name = $n;}
public function getAge($g) {return $this->age = $g;}
public function getTall($t) {return $this->tall = $t;}
}


</body>

